<Page xmlns="http://schemas.nativescript.org/tns.xsd" navigatingTo="onNavigatingTo" class="page">
    <Page.actionBar>
        <ActionBar title="Tic Tac Toe Greens" class="action-bar">
            <android>
                <NavigationButton icon="res://icon" tap="goBack" />
            </android>
            <ios>
                <ActionItem icon="res://icon" ios.position="left" tap="goBack" />
            </ios>
        </ActionBar>
    </Page.actionBar>

    <GridLayout columns="*" rows="auto">
        <StackLayout col="0" row="0">
            <Label text="Settings" class="h1"/>
        </StackLayout>
    </GridLayout>
</Page>